TRASH       = _{ (!(FAILED_TEST | FAILED_STEP) ~ ANY) }
FAILED_TEST = _{ "FAILED: " }
FAILED_STEP = _{ "Ã— " }

FAILED_STEP_MSG = { (!"flutter: " ~ ANY)+ }

